<%= form_for(@prjct) do |f| %>
	<div class="form-group">
		<%= f.label :title %>
		<%= f.text_field :title, class: 'form-control' %>
	</div>

	<div class="form-group">
		<%= f.label :description %>
		<%= f.text_area :description, class: 'form-control' %>
	</div>

	<div class="form-group">
		<%= f.label 'date' %>
		<%= f.date_field :deight, class: 'form-control' %>
	</div>

	<div class="actions">
    	<%= f.submit 'Create Project', class: 'btn-new' %>
  	</div>
<% end %>